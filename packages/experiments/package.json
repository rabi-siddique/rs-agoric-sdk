{
  "name": "agoric-contract-experiments",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:chain": "docker run -d --name agoric -p 26657:26657 -p 1317:1317 -p 9090:9090 ghcr.io/agoric/agoric-3-proposals:latest",
    "build": "agoric run src/builder.js",
    "deploy": "yarn build && runInsideContainer=true planFile=startContract-plan.json node deploy.js",
    "bundles": "agoric run ../builders/scripts/orchestration/axelar-gmp.build.js --net=localhost --peer=axelar:connection-0:channel-0:uaxl",
    "build:contract": "cd ../orchestration && yarn esbuild:axelar && cd ../experiments && yarn bundles",
    "build:token": "agoric run ../../multichain-testing/src/register-interchain-bank-assets.builder.js --assets='[{\"denom\":\"ibc/2CC0B1B7A981ACC74854717F221008484603BB8360E81B262411B0D830EDE9B0\",\"issuerName\":\"AXL\",\"decimalPlaces\":6}]'",
    "deploy-axl": "yarn build:contract && runInsideContainer=true planFile=startAxelarGmp-plan.json node deploy.js",
    "deploy:token": "yarn build:token && runInsideContainer=true planFile=eval-register-interchain-bank-assets-plan.json node deploy.js"
  },
  "dependencies": {
    "@agoric/deploy-script-support": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/vat-data": "workspace:*",
    "@agoric/xsnap": "workspace:*",
    "@agoric/zoe": "workspace:*",
    "@endo/errors": "^1.2.10",
    "@endo/eventual-send": "^1.3.1",
    "@endo/far": "^1.1.11",
    "@endo/init": "^1.1.9",
    "@endo/marshal": "^1.6.4",
    "@endo/pass-style": "^1.5.0",
    "@endo/patterns": "^1.5.0",
    "ava": "5.3.0"
  },
  "devDependencies": {
    "execa": "^9.6.0",
    "typescript": "~5.8.2"
  },
  "keywords": [],
